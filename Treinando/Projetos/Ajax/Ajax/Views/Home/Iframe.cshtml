@{
    ViewBag.Title = "Iframe";
}

<h2>Carregamento assíncrono de iframe</h2><br/>
<div id="facebook">
 </div>

@section scripts {
    <script>
        $(window).load(function () {
            $('div#facebook')
              .html('<iframe src="//www.facebook.com/plugins/likebox.php?href=https%3A%2F%2Fwww.facebook.com%2FFacebookDevelopers" scrolling="no" frameborder="0" style="border:none; overflow:hidden; height:290px;" allowtransparency="true"></iframe>');
        });
    </script>
}